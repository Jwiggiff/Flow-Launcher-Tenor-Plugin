		"eslint": "^8.57.0",
		"husky": "^8.0.3",
		"lint-staged": "^13.3.0",
		"prettier": "^2.8.8",
		"typescript": "^4.9.5"
	},
	"dependencies": {
		"flow-launcher-helper": "^2.2.0",
		"winston": "^3.13.0",
		"zod": "^3.23.8"
